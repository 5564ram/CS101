#include<stdio.h>

int main()
{
    int n,pos=0,neg=0,zero=0;
    scanf("%d",&n);
    int a[n];
    for(int i=1; i<=n; ++i)
    {
      scanf("%d",&a[i]);
      if(a[i]>0){pos=pos+1;}
      else if(a[i]<0){neg=neg+1;}
      else if(a[i]==0)zero=zero+1;}
      }
      printf("%f\n",pos/n);
      printf("%f\n", neg/n);
      printf("%f\n", zero/n);
      return 0;
}

